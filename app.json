{
  "expo": {
    "name": "rebu",
    "slug": "rebu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danengel.rebu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This is a rideshare app that is built in React Native. The authors are Brendan Smith, Yulia Barysevich, Ceylin Brooks, and Dan Engel. We built this app in a week-long sprint for our Code Fellows final project.",
    "githubUrl": "https://github.com/CeylinBrooks/RebuNative"
  }
}
